{
  "name": "@ehacke/prettier-config",
  "description": "Node-focused prettier config",
  "version": "2.0.0",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/ehacke/prettier-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@ehacke/commitlint-config": "^3.0.0",
    "commitizen": "^4.3.1",
    "commitlint": "^11.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "fixpack": "^3.0.6",
    "husky": "^4.3.8"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/ehacke/prettier-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "prettier"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "prettier": "^2.8.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehacke/prettier-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "major": "npm version major && npm publish",
    "minor": "npm version minor && npm publish",
    "patch": "npm version patch && npm publish",
    "postpublish": "git push",
    "prepare": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
